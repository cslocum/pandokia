wdir=/eng/ssb/pandokia_dev/pandokia


if [ `pwd` != $wdir ]
then
	echo wrong directory
	exit 1
fi
echo ok

rm -rf log

for x in arzach banana bond cadeau herbert thor ssb
do
	echo XXXXX
	echo $x
	ssh $x "cd $wdir && ./go iraf > log/`hostname`.iraf && ./go irafx > log/`hostname`.irafx && ./go irafdev> log/`hostname`.irafdev "
done

echo XXXXX
echo arzach again - earlier python versions
ssh arzach "cd $wdir && ./go irafdev 2.5.4 > log/`hostname`/irafdev.2.5.4 && ./go irafdev 2.6.5> log/`hostname`/irafdev.2.6.5 "

